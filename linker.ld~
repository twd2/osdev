 OUTPUT_ARCH(i386:x86-64) */
ENTRY(_start)

SECTIONS
{
  . = 0x00100000;

  .text ALIGN(4K):
  {
    *(.text)
  }

  .rodata ALIGN(4K):
  {
    *(.rodata*)
  }

  .data ALIGN(4K):
  {
    *(.data)
  }

  .bss ALIGN(4K):
  {
    *(COMMON)
    *(.bss)
  }

  .gdt32 ALIGN(4K):
  {
    *(.gdt32)
  }
}
